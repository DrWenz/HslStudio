<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:HslStudio.Scada.ViewModels"
			 xmlns:local="using:HslStudio.HslControls"
			 xmlns:localFeed="using:HslStudio.HslControls.Feed"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="HslStudio.Scada.Views.MainView"
             Foreground="White"
             Background="#171C2C">
  <Design.DataContext>
    <viewModels:MainViewModel />
  </Design.DataContext>
	<StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
		<local:LedSingle Height="100" Width="100" OnColor="green" LedStyle="Circle"/>
		<local:LedSingle Height="100" Width="100" OnColor="green" LedStyle="Rectangle" Value="True"/>
		<local:LedSingle Height="100" Width="100" OnColor="green" LedStyle="Triangle" Value="True"/>

	</StackPanel>
	 
	
	 
</UserControl>
